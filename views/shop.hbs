         <section >
           <div class="container">
    		<div class="row">
            {{#each products}}
    			<div class="col-md-4 ftco-animate">
    				<div class="product">
    					<a href="#" class="img-prod"><img class="img-fluid" src="/product-images/{{this._id}}.jpg"  style="width:250px; height:250px ;"  alt="Colorlib Template">
    						<span class="status"></span>
    						<div class="overlay"></div>
    					</a>
    					<div class="text py-3 pb-4 px-3 text-center">
    						<h3><a href="#">{{this.name}}</a></h3>
                <h4><a href="#">{{this.category}}</a></h4>
                <h5><a href="#">{{this.description}}</a></h5>
    						
    							{{#if status}}
                  <span class="status">Limited offer!</span>
                  <p class="price"><span class="mr-2 price-dc"></span><span class="price-sale">Rs.{{this.disPrice}}</span></p>

                  {{else}}
		    						<p class="price"><span class="mr-2 price-dc"></span><span class="price-sale">Rs.{{this.price}}</span></p>
                    {{/if}}
                    {{!-- <a href="/add-to-cart/{{this._id}}" class="btn btn-success" >Add to cart</a> --}}
                    <button class="btn btn-success" id="sweetalert" onclick="addToCart('{{this._id}}')">Add to Cart</button>
		    					</div>
	    					
    				
    				</div>
    			</div>
    		
    			
    			
    			 {{/each}}	
    		</div>
        </div>
        </section>
       

        <script>
          function addToCart(proId){
            $.ajax({
              url:'/add-to-cart/'+proId,
              method:'get',
              success:(response)=>{
                if(response.status){
                 
          swal("Product added!", "product successfully added to cart!", "success");

        
                  
                  let count=$('#cart-count').html()
                  count=parseInt(count)+1
                  $("#cart-count").html(count)

                }
              
              }
            })
          }
        </script>
